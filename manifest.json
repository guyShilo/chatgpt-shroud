{
  "manifest_version": 3,
  "name": "ChatGPT Shroud",
  "description": "This extension will hide the chat history on chat.openai.com",
  "version": "1.0",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_security_policy": {}
}
